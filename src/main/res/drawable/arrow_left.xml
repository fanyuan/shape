<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:bottom="11.5dp">
        <rotate
            android:fromDegrees="315"
            android:pivotX="45%"
            android:pivotY="45%"
            android:toDegrees="0">
            <shape>
                <solid android:color="#ff00ff" />
                <size
                    android:width="2dp"
                    android:height="2dp" />
                <stroke
                    android:width="1dp"
                    android:color="#fff000ff" />
            </shape>
        </rotate>
    </item>
    <item android:top="11.5dp">
        <rotate
            android:fromDegrees="45"
            android:pivotX="45%"
            android:pivotY="45%"
            android:toDegrees="0">
            <shape>
                <solid android:color="#ffffff" />
                <size
                    android:width="20dp"
                    android:height="2dp" />
                <stroke
                    android:width="1dp"
                    android:color="#fffff00f" />
            </shape>
        </rotate>
    </item>
</layer-list>
